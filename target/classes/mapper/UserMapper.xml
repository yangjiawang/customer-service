<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dehe.mapper.UserMapper">



    <select id="selectuser" resultType="com.dehe.domain.User">
        select * from usertable where  username=#{username}
    </select>

    <select id="selectiptable" resultType="java.lang.Integer">
        select frequency from iptabel where ipaddress = #{ipaddress}
    </select>





</mapper>